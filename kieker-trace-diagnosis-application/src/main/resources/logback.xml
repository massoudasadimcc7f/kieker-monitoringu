<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">

	<statusListener class="ch.qos.logback.core.status.NopStatusListener" />

	<appender name="TeeTimeFile" class="ch.qos.logback.core.FileAppender">
		<file>logs/TeeTime.log</file>
		<encoder>
			<pattern>%d %p %C [%t] %m%n</pattern>
		</encoder>
	</appender>

	<root level="warn">
		<appender-ref ref="TeeTimeFile" />
	</root>

</configuration> 